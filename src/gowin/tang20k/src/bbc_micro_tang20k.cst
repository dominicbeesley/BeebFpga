//Locations suitable for 20K primer in ext dock board

// System Clock (27MHz)
IO_LOC  "sys_clk" H11;
IO_PORT "sys_clk" IO_TYPE=LVCMOS33 PULL_MODE=NONE;

// Button 1 - S0 - silicone key_1
IO_LOC  "btn1_n" T10;	
IO_PORT "btn1_n" IO_TYPE=LVCMOS33 PULL_MODE=NONE;

// Button 2 - S1 - silicone key_2
IO_LOC  "btn2_n" T3;
IO_PORT "btn2_n" IO_TYPE=LVCMOS33 PULL_MODE=NONE;

// Keyboard - PMOD1
IO_LOC  "ps2_clk" T11;
IO_PORT "ps2_clk" IO_TYPE=LVCMOS33 PULL_MODE=UP;
IO_LOC  "ps2_data" P11;
IO_PORT "ps2_data" IO_TYPE=LVCMOS33 PULL_MODE=UP;

// Mouse - PMOD1
IO_LOC  "ps2_mouse_clk" T12;
IO_PORT "ps2_mouse_clk" IO_TYPE=LVCMOS33 PULL_MODE=UP;
IO_LOC  "ps2_mouse_data" R11;
IO_PORT "ps2_mouse_data" IO_TYPE=LVCMOS33 PULL_MODE=UP;

// VGA - pmod0
IO_LOC "vga_vs" T6;
IO_PORT "vga_vs" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC "vga_hs" P6;
IO_PORT "vga_hs" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC "vga_r" T7;
IO_PORT "vga_r" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC "vga_g" R8;
IO_PORT "vga_g" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC "vga_b" T8;
IO_PORT "vga_b" IO_TYPE=LVCMOS33 PULL_MODE=NONE;

IO_LOC "hsync_org" T9;
IO_PORT "hsync_org" IO_TYPE=LVCMOS33 PULL_MODE=NONE;

IO_LOC "clock27_o" P9;
IO_PORT "clock27_o" IO_TYPE=LVCMOS33 PULL_MODE=NONE;


// HDMI
IO_LOC "tmds_d_p[0]" H14,H16;
IO_PORT "tmds_d_p[0]" PULL_MODE=NONE;
IO_LOC "tmds_d_p[1]" J15,K16;
IO_PORT "tmds_d_p[1]" PULL_MODE=NONE;
IO_LOC "tmds_d_p[2]" K14,K15;
IO_PORT "tmds_d_p[2]" PULL_MODE=NONE;
IO_LOC "tmds_clk_p" G16,H15;
IO_PORT "tmds_clk_p" PULL_MODE=NONE;

IO_LOC  "AUD_DACLRCK" P16;
IO_PORT "AUD_DACLRCK" IO_TYPE=LVCMOS33;
IO_LOC  "AUD_BCLK" N15;
IO_PORT "AUD_BCLK" IO_TYPE=LVCMOS33;
IO_LOC  "AUD_DACDAT" P15;
IO_PORT "AUD_DACDAT" IO_TYPE=LVCMOS33;
IO_LOC  "AUD_EN" R16;
IO_PORT "AUD_EN" IO_TYPE=LVCMOS33;


// SD Card
IO_LOC  "tf_cs" N11;
IO_PORT "tf_cs" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC  "tf_mosi" R14;
IO_PORT "tf_mosi" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC  "tf_sclk" N10;
IO_PORT "tf_sclk" IO_TYPE=LVCMOS33 PULL_MODE=NONE;
IO_LOC  "tf_miso" M8;
IO_PORT "tf_miso" IO_TYPE=LVCMOS33 PULL_MODE=NONE;

// UART
IO_LOC  "uart_tx" M11;
IO_PORT "uart_tx" IO_TYPE=LVCMOS33;
IO_LOC  "uart_rx" T13;
IO_PORT "uart_rx" IO_TYPE=LVCMOS33;

// LEDs
IO_LOC  "led[5]" L16;
IO_PORT "led[5]" PULL_MODE=UP IO_TYPE = LVCMOS33;
IO_LOC  "led[4]" L14;
IO_PORT "led[4]" PULL_MODE=UP IO_TYPE = LVCMOS33;
IO_LOC  "led[3]" N14;
IO_PORT "led[3]" PULL_MODE=UP IO_TYPE = LVCMOS33;
IO_LOC  "led[2]" N16;
IO_PORT "led[2]" PULL_MODE=UP IO_TYPE = LVCMOS33;
IO_LOC  "led[1]" A13;
IO_PORT "led[1]" PULL_MODE=UP IO_TYPE = LVCMOS33;
IO_LOC  "led[0]" C13;
IO_PORT "led[0]" PULL_MODE=UP IO_TYPE = LVCMOS33;

// SPI Flash
IO_LOC  "flash_cs" M9;
IO_PORT "flash_cs" IO_TYPE=LVCMOS33;
IO_LOC  "flash_ck" L10;
IO_PORT "flash_ck" IO_TYPE=LVCMOS33;
IO_LOC  "flash_si" R10;
IO_PORT "flash_si" IO_TYPE=LVCMOS33;
IO_LOC  "flash_so" P10;
IO_PORT "flash_so" IO_TYPE=LVCMOS33 PULL_MODE=UP;
